    <script>
      $(document).ready(function() {
        let prev=0
      let next = 0
      let pos= 0
      $('.prev').click(function () {
        prev+=1
        pos-=100
        // console.log(pos)
        $(".item").css('transform', 'translate('+ pos +'px, 0)')
      })
      $('.next').click(function () {
        next+=1
        pos+=50
        $(".item").css('transform', 'translate('+ pos +'px, 0)')
      })
});
    </script>
   
