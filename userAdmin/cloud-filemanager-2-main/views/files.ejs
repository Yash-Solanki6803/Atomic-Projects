  <body class="stretched">
  <!-- <section> -->
    <div id="wrapper" class="clearfix">
        <%- include('header'); -%>

        <div class="timeline">
          <a class="next navigation"><i class="fas fa-arrow-left"></i></a>
          <div class="timeline-slider">

            <% folders.map(folder=>{ %>

              <div class="item">
                <a href="/download/<%= folder._id %>">
                <div id="<%= folder._id %>" class="node" title="<%= folder.desc %>" style="background-image: url('/files/thumbnail/<%= folder.img%>');"><i class="far fa-file"></i>
                </a>
                  <div class="date"><%= folder.name %><br><%= folder.date.toLocaleDateString('en-us', { year:"numeric", month:"short", day:"numeric"}) %></div> </div>
              <div class="line"></div>
              <div class="line"></div>
    
            </div>

              <% }) %>
          
       

          </div>
          <a class="prev navigation"><i class="fas fa-arrow-right"></i></a>
   </div>
      <!-- </section> -->
      <div class="description">
        <h1>Description</h1>
        Lorem ipsum dolor sit, amet consectetur adipisicing elit. Quas, alias earum. Alias!
      </div>
      <%- include('footer'); -%>

    </div>

