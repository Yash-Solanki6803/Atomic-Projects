      >
        <div class="container clearfix">
          <div
            id="contact-form-overlay-mini"
            class="clearfix"
            style="width: 50%"
          >
            <div class="fancy-title title-dotted-border">
              <h3>Login</h3>
            </div>
            <div class="form-widget">
              <div class="form-result"></div>

              <form
                class="nobottommargin"
                action="/api/login"
                method="POST"
                enctype="application/x-www-form-urlencoded"
              >
               <div class="col_full">
                  <label for="template-contactform-email"
                    >Username <small>*</small></label
                  >
                  <input
                    type="text"
                    id="template-contactform-email"
                    name="username"
                    value=""
                    class="required  sm-form-control"
                  />
                </div>
                <div class="clear"></div>

                <div class="clear"></div>
                <div class="col_full">
                  <label for="template-contactform-subject"
                    >Password <small>*</small></label
                  >
                  <input
                    type="password"
                    id="template-contactform-subject"
                    name="password"
                    
                    class="required sm-form-control"
                  />
                </div>

              
                <div class="col_full">
                  <button
                    style="background-color: black !important"
                    class="button button-3d nomargin"
                    type="submit"
                  >
                    Login
                  </button>
                </div>
                
              </form>
            </div>
          </div>
        </div>

        <!-- <section id="google-map" class="gmap"></section> -->
      </section>

      <%- include('footer'); -%>

