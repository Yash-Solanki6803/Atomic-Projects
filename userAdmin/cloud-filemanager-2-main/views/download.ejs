    <style>
      .fbox-icon>a>i{
        color:black !important;
      }
      .navigation{
        cursor: pointer;
        color: black;
        padding: 8px;
        width: 50px;
        height: 50px;
        display: flex;
        align-items: center;
        justify-content: center;
        transition: 300ms;
        font-size: 18px;
      }
      .navigation:hover{
        background-color: #79E0EE;
        
        border-radius: 50%;
      }
      .col-md-3{
        margin: 2%  0;
        transition: 0.4s;
      }

      .col-md-3:hover{
        transform: scale(1.1);
      }
    </style>
