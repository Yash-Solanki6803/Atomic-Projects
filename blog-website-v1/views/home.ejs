<%- include('partials/header'); -%>

<h1>home</h1>
<p><%=StartingContent%></p>

<%posts.forEach(function(post){%>
    <h1><%=post.postTitle%></h1>
    <p>
        <%=post.mainContent.substring(0,10) + "..."%>
    </p>
    <p>
        <%=post.subContent.substring(0,10) + "..."%>
        <a href="/posts/<%=post.postTitle%>">Read more</a>
    </p>
    
<%});%>

<%- include('partials/footer'); -%>